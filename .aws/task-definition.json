{
    "containerDefinitions": [
        {
            "name": "owasp-juice-shop", 
            "image": "nginx:latest", 
            "portMappings": [
                {
                    "containerPort": 3000, 
                    "hostPort": 300, 
                    "protocol": "tcp"
                }
            ]
        }
    ], 
    "memory": "512",
    "cpu": "256", 
    "networkMode": "awsvpc", 
    "family": "http-server", 
    "requiresCompatibilities": [
        "FARGATE"
    ]
}